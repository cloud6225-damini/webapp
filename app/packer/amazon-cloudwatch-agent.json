{
    "logs": {
      "force_flush_interval": 5,
      "logs_collected": {
        "files": {
          "collect_list": [
            {
              "file_path": "/var/log/webapp.log",
              "log_group_name": "webapp-log-group",
              "log_stream_name": "{instance_id}-webapp",
              "timestamp_format": "%Y-%m-%d %H:%M:%S"
            }
          ]
        }
      }
    },
    "metrics": {
      "namespace": "WebAppMetrics",
      "metrics_collected": {
        "statsd": {
          "service_address": ":8125"
        }
      }
    }
}